algoritmo "Imposto"
//Creuza comprou muitas muabas e deve pagar imposto de 60% do valor
var
   preco,imposto:real
inicio
      Escreva ("Qual o valor do preço do produto? US$: ")
      leia(preco)
      imposto<-(preco*60)/100//assim se calcula percentagem %
      
      Escreva("O imposto será de US$",imposto)
fimalgoritmo
