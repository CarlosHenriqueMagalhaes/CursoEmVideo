algoritmo "ConteAteOndeQuiser"
//realizar contagem de 0 até onde o usuário quiser
var
   cont,valor:inteiro
inicio
      cont<-0
      escreva("deseja contar até quanto? ")
      leia(valor)
      
      enquanto (cont<=valor) faca
         Escreval(cont)
         cont<-cont+1
      fimenquanto
      Escreva ("Terminei de contar!")
fimalgoritmo
      
      

