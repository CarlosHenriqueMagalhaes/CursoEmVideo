algoritmo "OperadoresLogicos
//Colocar condição E, OU e retornar verdadeiro ou falso
var
   A,B,C:real
inicio
      escreva ("Digite um valor para A: ")
      leia (A)
      escreva ("Digite um valor para B: ")
      leia (B)
      escreva ("Digite um valor para C: ")
      leia (C)

      escreval ((A=B) ou (C>A)) //Brinque alterando OU e E
      escreval ((A=B) e (C>A))
fimalgoritmo
